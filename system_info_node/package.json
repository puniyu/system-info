{
  "name": "@puniyu/system-info",
  "version": "1.5.2",
  "description": "一个用于获取系统信息库",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "author": "puniyu",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/puniyu/system-info.git"
  },
  "license": "MIT",
  "keywords": [
    "NAPI",
    "napi-rs",
    "system",
    "system-info"
  ],
  "files": [
    "dist"
  ],
  "napi": {
    "binaryName": "system-info",
    "targets": [
      "x86_64-pc-windows-msvc",
      "x86_64-apple-darwin",
      "x86_64-unknown-linux-gnu",
      "aarch64-unknown-linux-gnu",
      "aarch64-apple-darwin"
    ],
    "dtsHeader": "// powered by system_info_core\n"
  },
  "engines": {
    "node": ">= 12.22.0 < 13 || >= 14.17.0 < 15 || >= 15.12.0 < 16 || >= 16.0.0"
  },
  "scripts": {
    "artifacts": "napi artifacts",
    "build": "napi build --platform --release -p system_info_node --esm -o dist",
    "prepublishOnly": "napi prepublish --no-gh-release",
    "create-npm-dirs": "napi create-npm-dirs"
  },
  "devDependencies": {
    "@napi-rs/cli": "^3.4.1"
  },
  "packageManager": "pnpm@10.24.0"
}
